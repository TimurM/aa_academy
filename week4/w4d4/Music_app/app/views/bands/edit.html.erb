<h1> Edit this Band </h1>

<form action="<%= band_url(@band) %>" method="post">
  <input type="hidden" name="_method" value="PATCH">
  <input type="hidden"
         name="authenticity_token"
         value="<%= form_authenticity_token %>">

  <label for="band_name">Name</label>
  <input type="text"
  name="band[name]"
  id="band_name">

  <br>
  <input type = "submit">

</form>
